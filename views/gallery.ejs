<%- include('./partials/head') %>
    <style>/* CSS used here will be applied after bootstrap.css */
        .carousel {
            margin-top: 20px;
        }
       
        .item .thumb {
            width: 25%;
            cursor: pointer;
            float: left;
        }
        .item .thumb img {
            width: 95%;
            margin: 12px;
            height: 150px;
        }
        .item img {
            width: 100%;	
        }

        </style>
</head>
<body>
    <!-- ? Preloader Start -->

    <!-- Preloader Start -->
<%- include('./partials/navbar') %>
    <main>
        <!-- Slider Area Start-->
        <div class="slider-area slider-bg ">
            <!-- Single Slider -->
            <div class="single-slider d-flex align-items-center slider-height3">
                <div class="container">
                    <div class="row align-items-center justify-content-center">
                        <div class="col-xl-8 col-lg-9 col-md-12 ">
                            <div class="hero__caption hero__caption3 text-center">
                                <h1 data-animation="fadeInLeft" data-delay=".6s ">Gallery</h1>
                            </div>
                        </div>
                    </div>
                </div>
            </div>    
            <!-- Slider Shape -->
            <div class="slider-shape d-none d-lg-block">
                <img class="slider-shape1" src="/img/hero/top-left-shape.png" alt="">
            </div>
        </div>
        <!-- Slider Area End -->
        <div class="container">
            <div class="row " >
              <div class="col-lg-12">
                  <div id="carousel" class="carousel slide" data-ride="carousel">
                      <div class="carousel-inner" style="height:600PX">
                          <div class="item active">
                              <img src="/cr1.jpeg">
                          </div>
                          <div class="item">
                              <img src="/cr2.jpeg">
                          </div>
                          <div class="item">
                              <img src="/cr3.jpeg">
                          </div>
                          <div class="item">
                              <img src="/cr4.jpeg">
                          </div>
                          <div class="item">
                            <img src="/img/gallery/g1.jpg">
                        </div>
                        <div class="item">
                            <img src="/img/gallery/g2.jpg">
                        </div>
                        <div class="item">
                            <img src="/img/gallery/g3.jpg">
                        </div>
                          
                      </div>
                  </div> 
              <div class="clearfix">
                  <div id="thumbcarousel" class="carousel slide" data-interval="false">
                      <div class="carousel-inner">
                          <div class="item active">
                              <div data-target="#carousel" data-slide-to="0" class="thumb"><img src="/cr1.jpeg" ></div>
                              <div data-target="#carousel" data-slide-to="1" class="thumb"><img src="/cr2.jpeg"></div>
                              <div data-target="#carousel" data-slide-to="2" class="thumb"><img src="/cr3.jpeg"></div>
                              <div data-target="#carousel" data-slide-to="3" class="thumb"><img src="/cr4.jpeg"></div>

                          </div><!-- /item -->
                          <div class="item">
                            <div data-target="#carousel" data-slide-to="4" class="thumb"><img src="/img/gallery/g1.jpg"></div>
                            <div data-target="#carousel" data-slide-to="5" class="thumb"><img src="/img/gallery/g2.jpg"></div>
                            <div data-target="#carousel" data-slide-to="6" class="thumb"><img src="/img/gallery/g3.jpg"></div>
                            <div data-target="#carousel" data-slide-to="7" class="thumb"><img src="/img/gallery/g7.jpg"></div>
                        </div>

                      </div><!-- /carousel-inner -->
                      <a class="left carousel-control" href="#thumbcarousel" role="button" data-slide="prev">
                          <span class="glyphicon glyphicon-chevron-left"></span>
                      </a>
                      <a class="right carousel-control" href="#thumbcarousel" role="button" data-slide="next">
                          <span class="glyphicon glyphicon-chevron-right"></span>
                      </a>
                  </div> <!-- /thumbcarousel -->
              </div><!-- /clearfix -->
              </div> <!-- /col-sm-6 -->
             
            </div> <!-- /row -->
          </div> <!-- /container -->
     
    </main>
<%- include('./partials/footer') %>

