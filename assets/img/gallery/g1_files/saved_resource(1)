function getRelatedKeywords()
{var catid=$("#catid").val();var type=$("#type").val();var tmp_iwhat=encodeURIComponent(iwhat);var tmp_related_keyword_key=encodeURIComponent(related_keyword_key);var citi=mncity;$.ajax({url:WEBROOT+"functions/getRelatedKeywords.php?catid="+catid+"&type="+type+"&text="+tmp_iwhat+"&area="+res_area+"&key="+tmp_related_keyword_key+"&city="+citi+"&acity="+acity+"&syntyp="+syntyp+"&version="+VERSION,async:true,success:function(result)
{getRelatedKeywordsHtml(result,""+catid+"",type,""+iwhat+"","'"+related_keyword_key+"'");}});}
function trackEvent(category,action,label)
{var live=liveCheck();if(live)
{ga('b._trackEvent',category,action,label);fbq('track','Lead',{content_name:category,content_category:category,currency:'INR'});}}
function liveCheck()
{var l=0;var pos=DOMAIN.search('justdial.com');l=(pos==-1)?0:1;return l;}
function getRelatedKeywordsHtml(resp,catid,type,tmp_iwhat,tmp_related_keyword_key)
