{open_caller_popup_ln();}
else
{var now=new Date();var time=now.getTime();time+=3600*1000;now.setTime(time);currentPageUrl=document.location.toString();document.cookie='bkurl='+escape($.trim(currentPageUrl))+'; expires='+now.toGMTString()+'; path=/'+'; domain='+cookieondomain;document.cookie="pwd_popup=open;path=/;domain="+cookieondomain;window.location.href=DOMAIN+'doctors-vendor/Todays-Appointment';}}}else if(data.results[0].isLoginSpa==1){var now=new Date();var time=now.getTime();time+=3600*1000;now.setTime(time);currentPageUrl=document.location.toString();document.cookie='bkurl='+escape($.trim(currentPageUrl))+'; expires='+now.toGMTString()+'; path=/'+'; domain='+cookieondomain;document.cookie="pwd_popup=open;path=/;domain="+cookieondomain;window.location.href=DOMAIN+'spa-vendor/appointment/index';}
