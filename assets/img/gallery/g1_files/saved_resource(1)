{batPopupHtml(bttype,docid,btaid,workingTrue)}
else
{var btcid=$('#btdocid').val();var btname=$('#bt_name').val();var btmobile=$('#bt_mobile').val();var btemail=$('#bt_email').val();var btdate=$("#btdate").val();var bcapacity=$("#booking_capacity").val();var slot=$("#slot").val();var modifiy=$("#modifiy").val();if(btaid!='undefined'&&$('#'+btaid).text()!='')
{var bttime=$('#'+btaid).text();}
