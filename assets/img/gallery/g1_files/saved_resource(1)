function ngofun()
{var date=new Date();document.cookie='inweb_what='+document.index.what.value+'; '+date+'; path=/; domain='+cookieondomain;document.cookie='inweb_city='+$("#city").val().trim()+'; '+date+'; path=/; domain='+cookieondomain;$.ajax({url:WEBROOT+"webmain/ajxmain.php?city="+$("#city").val()+"&cases=citycheck",async:false,success:function(result){if(result=="City Failed")
{$("#city").focus();alert("Please Select Valid City");return false;}
else
{$('<form action="'+$('#nst').val().replace('URLDYNCITY',$('#city').val().replace(/[.,\/]+/g,''))+'"></form>').appendTo('body').submit();}}});}
function getCookie(c_name){if(document.cookie.length>0){var c_start=document.cookie.indexOf(" "+c_name+"=");if(c_start!=-1){c_start=c_start+c_name.length+2;var c_end=document.cookie.indexOf(";",c_start);if(c_end==-1)
c_end=document.cookie.length;var cvalue=myunescape(document.cookie.substring(c_start,c_end));return unescape(cvalue);}}
return"";}
function myunescape(str)
{str=""+str;while(true)
{var i=str.indexOf('+');if(i<0)
break;str=str.substring(0,i)+'%20'+
str.substring(i+1,str.length);}
return unescape(str);}
function ed(id){return document.getElementById(id);}
