function getCookieByName(cname){var name=cname+"=";var decodedCookie=decodeURIComponent(document.cookie);var ca=decodedCookie.split(';');for(var i=0;i<ca.length;i++){var c=ca[i];while(c.charAt(0)==' '){c=c.substring(1);}
if(c.indexOf(name)==0){return c.substring(name.length,c.length);}}
return"";}
function getCookie(c_name){if(document.cookie.length>0){var c_start=document.cookie.indexOf(" "+c_name+"=");if(c_start!=-1){c_start=c_start+c_name.length+2;var c_end=document.cookie.indexOf(";",c_start);if(c_end==-1)c_end=document.cookie.length;var cvalue=myunescape(document.cookie.substring(c_start,c_end));return unescape(cvalue);}}
return"";}
function myunescape(str){str=""+str;while(true)
{var i=str.indexOf('+');if(i<0)
break;str=str.substring(0,i)+'%20'+
str.substring(i+1,str.length);}
return unescape(str);}
