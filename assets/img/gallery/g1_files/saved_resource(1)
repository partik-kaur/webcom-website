function loadfirstrelphotos(){$("a.lazy").each(function(){var self=this;var $self=$(self);var settings={effect:"fadeIn"};self.loaded=false;var original=$self.attr("data-original");if($.inArray(original,firstrelphotos)>-1){$self.css("background-image","url('"+original+"')");$self[settings.effect](settings.effect_speed);$self.css("background-image","url('"+original+"')");self.loaded=true;}});loadrelphotos();}
function loadrelphotos(){$("a.lazynew").each(function(){var self=this;var $self=$(self);var settings={effect:"fadeIn"};self.loaded=false;var original=$self.attr("data-original");if($.inArray(original,relphotos)>-1){$self.css("background-image","url('"+original+"')");$self[settings.effect](settings.effect_speed);$self.css("background-image","url('"+original+"')");self.loaded=true;}});}
function setsescookie(docid,vfdisp)
{var ftdarr={};ftdarr[docid]=vfdisp;var ftdarr1=array2json(ftdarr);setCookiebest('vfdisp',ftdarr1);}
function fProdReview(prid)
{if(getCookie('attn_user')!='logout'&&prid!='')
{$.get(DOMAIN+"/functions/product_reviews_initial.php",{prid:prid,case1:'fReview',city:$('#city').val()},function(data){});}}
function array2json(arr){var parts=[];var is_list=(Object.prototype.toString.apply(arr)==='[object Array]');for(var key in arr){var value=arr[key];if(typeof value=="object"){if(is_list)
parts.push(array2json(value));else
parts[key]=array2json(value);}else{var str="";if(!is_list)
str='"'+key+'":';if(typeof value=="number")
str+=value;else if(value===false)
str+='false';else if(value===true)
str+='true';else
str+='"'+value+'"';parts.push(str);}}
var json=parts.join(",");if(is_list)
return'['+json+']';return'{'+json+'}';}
function _ct(a,d,v,i){a=escape(a);d=escape(d);v=escape(v);var dtdtl=(typeof i!='undefined'&&i!='undefined'&&i!=null)?i:'';var url=DOMAIN+"functions/click_tracker.php?city="+$("#city").val()+"&li="+a+"&ll="+d+"&ver="+v+"&rnd="+Math.random()+"&dcid="+encodeURIComponent(dtdtl)+"&t="+encodeURIComponent(Date())+"&source=web";;if(v=='bus')
{if(getCookie('attn_user')=='logout')
{url=DOMAIN+"functions/click_tracker.php?mobile=&li="+a+"&ll="+d+"&ver="+v+"&rnd="+Math.random()+"&dcid="+encodeURIComponent(dtdtl)+"&t="+encodeURIComponent(Date())+"&source=web";;}
else
{url=DOMAIN+"functions/click_tracker.php?mobile="+getCookie('inLogMobile')+"&li="+a+"&ll="+d+"&ver="+v+"&rnd="+Math.random()+"&dcid="+encodeURIComponent(dtdtl)+"&t="+encodeURIComponent(Date())+"&source=web";;}}
if(v=='32')
{if(getCookie('attn_user')=='logout')
{loginmob='';loginname='';}
else
{loginmob=getCookie('inLogMobile');loginname=getCookie('inLogName');}
url=DOMAIN+"functions/click_tracker.php?mobile="+loginmob+"&name="+loginname+"city="+$("#city").val()+"&li="+a+"&ll="+d+"&ver="+v+"&rnd="+Math.random()+"&dcid="+encodeURIComponent(dtdtl)+"&t="+encodeURIComponent(Date())+"&source=web";}
$.ajax({url:url,async:(a=='hkpop')?true:false});}
