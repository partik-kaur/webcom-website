{var catid=$("#mnCatid").val();var citi=encodeURIComponent(getCookie('scity'));var ishotl=htl_flg;var nctpas=(typeof frNct!='undefined'&&frNct!='undefined'&&frNct!=null)?frNct:'';$.ajax({url:DOMAIN+"functions/isBExists.php?b_path="+banner_nwpath+"&b_img="+banner_img+"&htlis="+ishotl+"&b_link="+banner_link+"&b_owner="+banner_owner+"&b_detail_id="+banner_detail_id+"&b_id="+banner_id+"&catid="+catid+"&city="+citi+"&dcids="+$('#review_paid_id').val()+"&ncatid="+nctpas,async:true,success:function(result)
