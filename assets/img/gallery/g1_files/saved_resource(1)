function openOffer(docid,value,type){var html="";$('.offersdiv').html("");$.ajax({url:DOMAIN+"functions/offerDetails.php",type:"post",async:true,dataType:"json",data:{'docid':docid,'offerid':value,'type':type},success:function(result)
{html+="<ul class='offersdivul'>";html+="<li class='offerheading'><b>OFFERS</b></li>";if(result.coupon_title!=""){html+="<li class='schild'>"+result.coupon_title+"</li>";}
html+="<li>"+result.coupon_desc+"</li>";html+="<li class='offerPeriod'><span class='innerdiv'>Offer Period : "+result.Period+"</span></li>";html+="</ul>";if(type=='card'){if($.trim(result.coupon_term).length>0){html+="<ul class='offersdivul'>";html+="<li class='offerheading'><b>Deal Terms</b></li>";for(var i=0;i<result.coupon_term.length;i++)
{html+="<li>"+result.coupon_term[i]+"</li>";}
html+="</ul>";}}
if(type=='list'){if(result.highlights.length>0){html+="<ul class='offersdivul'>";html+="<li class='offerheading'><b>Deal Highlights</b></li>";for(var i=0;i<result.highlights.length;i++)
{html+="<li><i class='checkic'></i> "+result.highlights[i].text+"</li>";}
html+="</ul>";}
if(result.coupon_term.length>0){html+="<ul class='offersdivul offersex'>";html+="<li class='offerheading'><b>Deal Terms</b></li>";for(var i=0;i<result.coupon_term.length;i++)
{html+="<li>"+result.coupon_term[i].text+"</li>";}
html+="</ul>";}}
$('.offersdiv').html(html);setTimeout(function(){openDiv("offersdiv");},300)}});}
