if(currentX>startX){swipeScop.goPrev(1);}
startTime=0;startX=0;}});}}
$(document).ready(function(){photorating.swipeToChangeImage();if(pageName=='photorating_tblt'){photorating.openGall(1,0,0);$(document).on("touchstart click","[id^='img_']",function(event){if(event.type=="touchstart"){}else if(event.type=="click"){photorating.openGall(1,0,Number(this.id.replace('img_','')));}});}else{$(document).keydown(function(e){if(e.keyCode==39&&$('#Photogalley').is(':visible')){photorating.goNext(2);return false;}
