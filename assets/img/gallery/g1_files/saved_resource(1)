t=setTimeout("$('#myCarousel').carousel();",duration-1000);});}
if(typeof resBan.htlp!=='undefined'&&resBan.htlp!='null'&&resBan.htlp!='undefined'){Object.keys(resBan['htlp']).map(function(value,key){if(resBan['htlp'][value]==null){}
