{review_rating_lp(data,type);var arr={"lng_frating":[]};$('.lng_frating').each(function(){if(this.innerHTML!=''){arr.lng_frating.push(this.innerHTML);}});if(typeof arr.lng_frating!=='undefined'&&arr.lng_frating.length>0&&(onloadFn=='Result'||onloadFn=='detailsPage')){var lng_frating=arr.lng_frating;var D_data=JSON.stringify(lng_frating);var nlang=getCookie('web_lang');if(nlang!='undefined'&&nlang!=''&&nlang!='eng'){trigger_lan_change(nlang,D_data,'fratings','result');}}}});}}
function getImageExtension(imgnm)
{var tmp_org_img=imgnm;var tmp_img=imgnm.split('/');tmp_img=tmp_img[tmp_img.length-1];tmp_img=tmp_img.split('.');return tmp_img[tmp_img.length-1];}
function getphotos(review_paid_id,pagename,type)
{$.ajax({url:DOMAIN+"functions/sortbyphotosnew.php?contractid="+review_paid_id+"&cases=citycheck&pagename="+pagename+"&v="+VERSION,async:true,success:function(result){getphotos_html(result,pagename,type);loadfirstrelphotos();}});}
function getphotos_html(data,pagename,type)
{var dimenwh=(pagename=='detail')?'width="69" height="51"':'';var d=eval('('+data+')');if(BTIMPH==1){var defaultUrl=BOTIMG+"icontent/clogobg2.gif";}else{var defaultUrl=protocl+"content.jdmagicbox.com/icontent/clogobg2.gif";}
